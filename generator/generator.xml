<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <!-- 数据库驱动包位置 -->
    <classPathEntry location="D:\project\springboot\generator\mysql-connector-java-3.1.13-bin.jar"/>
    <!--<classPathEntry location="C:\oracle\product\10.2.0\db_1\jdbc\lib\ojdbc14.jar" />-->
    <context id="DB2Tables" targetRuntime="MyBatis3">
        <!--用作分页查询-->
        <plugin type="org.mybatis.generator.plugins.RowBoundsPlugin">
        </plugin>
        <commentGenerator>
            <!--true : 去除注释，但是xml会重复   设置成false，会生成注释，重复生成时候xml不会重复                &lt;!&ndash;&ndash;&gt;-->
            <property name="suppressAllComments" value="false"/>
        </commentGenerator>
        <!-- mysql数据库链接URL、用户名、密码 -->
        <jdbcConnection driverClass="com.mysql.jdbc.Driver" connectionURL="jdbc:mysql://localhost:3306/maventest"
                        userId="root" password="root">
            <!--Oracle数据库使用 -->
            <!--<jdbcConnection driverClass="oracle.jdbc.driver.OracleDriver" connectionURL="jdbc:oracle:thin:@localhost:1521:orcl" userId="msa" password="msa">-->
        </jdbcConnection>
        <javaTypeResolver>
            <property name="forceBigDecimals" value="false"/>
        </javaTypeResolver>

        <!-- 生成模型的包名和位置 -->
        <javaModelGenerator targetPackage="com.example.bean.exampleBean"
                            targetProject="D:\project\springboot\src\main\java">
            <property name="enableSubPackages" value="true"/>
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>

        <!-- 生成的映射文件包名和位置 -->
        <sqlMapGenerator targetPackage="com.example.mapping.autoGeneratedMapping"
                         targetProject="D:\project\springboot\src\main\java">
            <property name="enableSubPackages" value="true"/>
        </sqlMapGenerator>
        <!-- 生成DAO的包名和位置 -->
        <javaClientGenerator type="XMLMAPPER" targetPackage="com.example.mapperInterface.autoGeneratedMapperInteger"
                             targetProject="D:\project\springboot\src\main\java">
            <property name="enableSubPackages" value="true"/>
        </javaClientGenerator>
        <!-- 要生成那些表(更改tableName和domainObjectName就可以) -->
        <!--<table tableName="module"  domainObjectName="Module">-->
        <!--<property name="useActualColumnNames" value="true"/>-->
        <!--</table>-->
        <!--<table tableName="userInfo"  domainObjectName="UserInfo">-->
        <!--<property name="useActualColumnNames" value="true"/>-->
        <!--</table>-->

        <!--<table tableName="logs"  domainObjectName="Logs">-->
        <!--<property name="useActualColumnNames" value="true"/>-->
        <!--</table>-->
        <table tableName="userinfo" domainObjectName="UserInfo">
            <!--useActualColumnNames ： true ：数据库是什么就是什么   false 驼峰原则-->
            <property name="useActualColumnNames" value="true"/>
        </table>

        <!--<table tableName="%">-->
        <!--&lt;!&ndash;useActualColumnNames ： true ：数据库是什么就是什么   false 驼峰原则&ndash;&gt;-->
        <!--<property name="useActualColumnNames" value="false"/>-->
        <!--</table>-->

        <!-- enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false"  -->
    </context>
</generatorConfiguration>